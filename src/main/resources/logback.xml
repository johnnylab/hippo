<?xml version="1.0" encoding="UTF-8"?>
<configuration>
 	<!-- 应用程序输出日志的路径 -->
	<property name="APPLICATION_LOG_FILE_HOME" value="/opt/logs" />
    <include resource="logback-common.xml"/>
	<root level="info">
		<appender-ref ref="APPLICATION_LOG" />
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>
